	<queryString>
		<![CDATA[SELECT  * ,
	(present_reading - b.initial_reading) cons
	,Date_format(Str_to_date(to_month, '%Y%m'), '%M %Y') bill_month
	,service_charge + meter_service_charge meter
	,c.cons_name AS cust_cons_name
	,c.house_no AS cust_house_no
	,replace(c.address, '\n', ' ') AS cust_address
	,c.city AS cust_city
	,c.pin_code AS cust_pin_code
,tc.min_kl
,(no_meter_amt * tc.min_kl)metcons
,no_meter_amt
,(
		SELECT m.meter_id
		FROM meter_details m
		WHERE m.id = c.meter_details_id
		) AS cust_meter_no
	,c.pipe_size AS cust_pipe_size
	,c.met_reading_dt AS cust_meter_read_dt
	,(
		SELECT street_name
		FROM street_master
		WHERE street_no = Substring(b.can, 3, 2)
			AND division_master_id = Substring(b.can, 1, 2)
		) street_name
	,b.surcharge AS val
FROM bill_full_details b
INNER JOIN cust_details c ON c.can = b.can
INNER JOIN tariff_category_master t ON t.id = c.tariff_category_master_id
INNER JOIN tariff_master tm ON tm.tariff_category_master_id=c.tariff_category_master_id
INNER JOIN tariff_charges tc ON tc.tariff_master_id=tm.id
WHERE b.id = $P{id} and tc.tariff_type_master_id=1]]>
	</queryString>
