	<queryString>
		<![CDATA[SELECT *,(SELECT present_reading - prev_reading from  bill_full_details      where id=$P{id})cubic_meter,

       Date_format(Str_to_date(to_month, '%Y%m'), '%M %Y') bill_month,
       service_charge + meter_service_charge          meter,
       c.cons_name                                         AS cust_cons_name,
       c.house_no                                          AS cust_house_no,
       c.address                                           AS cust_address,
       c.city                                              AS cust_city,
       c.pin_code                                          AS cust_pin_code,
       c.meter_no                                          AS cust_meter_no,
       c.pipe_size                                         AS cust_pipe_size,
      c.met_reading_dt                                  AS cust_meter_read_dt,
 (SELECT street_name
        FROM   street_master
        WHERE  street_no = Substring(b.can, 3, 2))      street_name,
       b.surcharge                                         AS val
FROM   bill_full_details b
       JOIN cust_details c
              ON c.can = b.can
       JOIN tariff_category_master t
              ON t.id = c.tariff_category_master_id
WHERE  b.id=$P{id}]]>
	</queryString>
